eOutlined,CloseCircleOutlined,CloseOutlined} from '@ant-design/icons';\r\n\r\n\r\n\r\nconst ProductCardInCheckOut = ({p})=>{\r\n  \r\n    const colors = ['Black', \"Brown\", \"Silver\", \"White\", 'Blue',\"Red\"];\r\n const dispatch = useDispatch();\r\n\r\n    const handleColorChange = (e)=>{\r\n        // update the local storage with selected color\r\n\r\n        let cart = [];\r\n        if(typeof window !== 'undefined'){\r\n            if(localStorage.getItem('cart')){\r\n                cart = JSON.parse(localStorage.getItem('cart'));\r\n            }\r\n            // i is index\r\n            cart.map((product,i)=>{\r\n                if(product._id === p._id)\r\n                {\r\n                   cart[i].color = e.target.value; \r\n                }\r\n\r\n            })\r\n            localStorage.setItem('cart',JSON.stringify(cart));\r\n\r\n           dispatch({\r\n               type: 'ADD_TO_CART',\r\n               payload: cart , \r\n           })\r\n\r\n        }\r\n\r\n    }\r\n\r\n    const handleQuantityChange =  (e)=>{\r\n        let count = e.target.value < 1 ? 1 : e.target.value;\r\n\r\n     if(count > p.quantity){\r\n         toast.error(`Max Available Quantity is ${p.quantity}`);\r\n         return;\r\n     }\r\n\r\n         // update the local storage with selected Quantity\r\n\r\n         let cart = [];\r\n         if(typeof window !== 'undefined'){\r\n             if(localStorage.getItem('cart')){\r\n                 cart = JSON.parse(localStorage.getItem('cart'));\r\n             }\r\n             // i is index\r\n             cart.map((product,i)=>{\r\n                 if(product._id === p._id)\r\n                 {\r\n                    cart[i].count = count; \r\n                 }\r\n \r\n             })\r\n             localStorage.setItem('cart',JSON.stringify(cart));\r\n \r\n            dispatch({\r\n                type: 'ADD_TO_CART',\r\n                payload: cart , \r\n            })\r\n \r\n         }\r\n\r\n    }\r\n\r\n\r\n    const handleRemove =()=>{\r\n        // console.log(p._id);\r\n\r\n          // Delete product from local storage\r\n\r\n          let cart = [];\r\n          if(typeof window !== 'undefined'){\r\n              if(localStorage.getItem('cart')){\r\n                  cart = JSON.parse(localStorage.getItem('cart'));\r\n              }\r\n              // i is index\r\n              // splice method's second arg is how many item from that index\r\n              cart.map((product,i)=>{\r\n                  if(product._id === p._id)\r\n                  {\r\n                     cart.splice (i,1)\r\n                  }\r\n  \r\n              })\r\n              localStorage.setItem('cart',JSON.stringify(cart));\r\n  \r\n             dispatch({\r\n                 type: 'ADD_TO_CART',\r\n                 payload: cart , \r\n             })\r\n  \r\n          }\r\n\r\n\r\n\r\n\r\n    }\r\n\r\n    return(\r\n\r\n        <tbody>\r\n            <tr>\r\n\r\n<td>\r\n   <div style={{width:'100px',height: 'auto'}}>\r\n       {p.images.length ? (\r\n           <ModalImage \r\n           small={p.images[0].url}\r\n           large={p.images[0].url}\r\n           />\r\n       ):\r\n        (\r\n            <ModalImage \r\n           small={laptop}\r\n           large={laptop}\r\n           />\r\n        )}\r\n   </div>\r\n</td>\r\n\r\n<td>\r\n   {p.title}\r\n</td>\r\n<td>\r\n${p.price}\r\n</td>\r\n<td>\r\n{p.brand}\r\n</td>\r\n<td>\r\n\r\n<select className='form-control'\r\n onChange={handleColorChange}\r\n name='color' id=''>\r\n     {p.color ? <option value={p.color}>\r\n         {p.color}\r\n     </option> :\r\n     <option>Select</option>\r\n     }\r\n\r\n     {colors.filter((c)=>c !==p.color).map((c)=>(\r\n         <option key={c}\r\n         value={c}>\r\n             \r\n             {c}</option>\r\n     ))}\r\n\r\n</select>\r\n\r\n</td>\r\n<td className='text-center'>\r\n\r\n<input className='form-control' type='number'\r\nvalue={p.count}\r\nonChange={handleQuantityC